pipeline {
  
  agent any {
    
    stages{
      
      stage("build") {
        
        steps {
          echo 'build the application'
        }
        
      }
      stage("Test") {
        
        steps {
          echo 'Test the application'
        }
        
      }
       stage("Deploy") {
        
        steps {
          echo 'Deploy the application'
        }
        
      }
      
    }
    
  }
  
}
